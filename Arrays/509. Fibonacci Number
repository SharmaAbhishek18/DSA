class Solution {//T.C->O(n^2) S.C->O(N)
    public int fib(int n) {
       if(n <= 1) return n;
       return fib(n-1) + fib(n-2); 
    }
}
APPROACH 2 ->>>>>>>> 

class Solution {//T.C->O(n) S.C->O(1)
    public int fib(int n) { // Bottom - up  Approach (Tabulation)
       if(n <= 1) return n;
       int a = 0 , b = 1;
       for(int  i = 2 ;i <= n ;i++){
        int c = a + b;
        a = b;
        b = c;
       }
       return b;
    }
}
