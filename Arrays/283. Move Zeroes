class Solution { //T.C=O(N) , S.C=0(1)
    public void moveZeroes(int[] nums) {
    int i = 0; //pointer 1
    for(int j = 0; j < nums.length;j++){
        if(nums[j] != 0){
            swap(nums,i,j);
            i++;
        }
    }
}
    public static void swap(int[]arr,int i,int j){
        int temp = arr[i];
        arr[i] = arr[j];
        arr[j] = temp;
    }
}

