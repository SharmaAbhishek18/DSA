//T.C:O(N) AND S.C O(1)
class Solution {
    public ListNode reverseList(ListNode head) {
      
    ListNode prev = null;
    ListNode curr = head;

    while(curr != null){
        ListNode next  = curr.next;
        curr.next = prev;
        prev = curr; 
        curr = next;
        }
    return prev;   
    }
}206. Reverse Linked List
