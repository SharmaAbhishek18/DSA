class Solution {
    public ListNode deleteMiddle(ListNode head) {
        if(head == null || head.next == null) return null;
        ListNode fast = head,slow=head,curr=head;
        while(fast != null && fast.next != null){
            curr = slow;
            slow = slow.next;
            fast = fast.next.next;
        }
        curr.next = slow.next;
        return head;
    }
}
