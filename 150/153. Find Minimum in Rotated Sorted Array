class Solution {//T.C -> O(log n) S.C -> O(1)
    public int findMin(int[] nums) {
        int start = 0;
        int end = nums.length-1;

        while(start < end){
            int mid = start + (end - start) / 2; 
            //checking if mid is greater than the right element
            if(nums[mid] > nums[end]) start = mid + 1;
            else end = mid;
        }
        return nums[start];
    }
}
