class Solution {//Time: O(logâ‚… n) â†’ because each division reduces n by factor of 5.
                //Space: O(1) â†’ only using res.
    public int trailingZeroes(int n) {
    int res = 0;//total count of trailing zeros
    while (n > 0) {
        n /= 5;
        res += n;
    }
    return res;
}}
