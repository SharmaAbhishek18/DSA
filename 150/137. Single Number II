BRUTE FORCE APPROACH->>>
class Solution {//T.C->O(N) and S.C->O(1)
    public int singleNumber(int[] nums) {
        Map<Integer,Integer>map = new HashMap<>();
        for(int n:nums){
            map.put(n,map.getOrDefault(n,0)+1);
        }
        for(int key : map.keySet()){
            if(map.get(key) == 1)return key;
        }
    return -1;
    }
}

class Solution {
    public int singleNumber(int[] nums) {
        int result = 0;
        for(int i = 0; i<32; i++) {
            int temp = (1 << i);
            int countOne  = 0;
            int countZero = 0;

            for(int num : nums) {
                if((num&temp) == 0) {
                    countZero++;
                } else {
                    countOne++;
                }
            }
         if(countOne % 3 == 1) {
                result = (result | temp);
            }
        }
        return result;
    }
};
