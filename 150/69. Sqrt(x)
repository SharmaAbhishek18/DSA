class Solution {//T.C->O(log x) S.C->O(1)
    public int mySqrt(int x) {
        long start = 1, end = x, ans = 0;
        while (start <= end) {
            long mid = start + (end - start) / 2;
            if (mid * mid == x) return (int) mid; //nearest integer ->(int)
            else if (mid * mid < x) {
                ans = mid;
                start = mid + 1;
            } else {
                end = mid - 1;
            }
        }
        return (int) ans;
    }
}
