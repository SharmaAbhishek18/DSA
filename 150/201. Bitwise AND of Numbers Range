class Solution {//T.C->O(log(n)) S.C->O(1)
    public int rangeBitwiseAnd(int left, int right) {
        int shiftCount = 0;
        while(left != right){
            left = (left >> 1);
            right = (right >> 1);
            shiftCount++;
        }
    return left << shiftCount;
    }
}

OPTIMAL SOL.
class Solution {//T.C->O(log(n)) S.C->O(1)
    public int rangeBitwiseAnd(int left, int right) {
        while(right > left){
            right = (right & (right-1));
        }
    return right;
    }
}

