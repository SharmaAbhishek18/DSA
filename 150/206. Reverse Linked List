//T.C:O(N) AND S.C O(N)
class Solution {
    public ListNode reverseList(ListNode head) {
      
        //USING STACK(LIFO)
       Stack<Integer>stack = new Stack<>();
       while(head != null){
       stack.push(head.val);
       head = head.next;
       }
       ListNode reversedList = new ListNode(Integer.MIN_VALUE);//-infinity
       ListNode ptr = reversedList;
    
       while(!stack.isEmpty()){
        ptr.next = new ListNode(stack.pop());
        ptr = ptr.next;
       }
        return reversedList.next;
    }
}
EFFICIENT SOL
//T.C:O(N) AND S.C O(1)
class Solution {
    public ListNode reverseList(ListNode head) {
      
      ListNode prev = null;
      ListNode curr = head;

      while(curr != null){
        ListNode next = curr.next;
        curr.next = prev;
        prev = curr;
        curr = next;
      }
    return prev;
    }
}
